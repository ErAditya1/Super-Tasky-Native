{
  "expo": {
    "name": "Super Tasky",
    "slug": "super-tasky",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/detailed_logo.jpg",
    "scheme": "supertasky",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "assets/fonts/*",
      "assets/images/*"
    ],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/detailed_logo.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.SuperTasky"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "SOCKET_URI": "https://super-tasky-server.onrender.com",
      "router": {},
      "eas": {
        "projectId": "eb4d0416-ecab-42c0-b791-d9acdb8545af"
      }
    },
    "owner": "eraditya1"
  }
}
